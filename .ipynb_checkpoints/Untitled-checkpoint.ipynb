   "source": [
    "def s(x,n):\n",
    "    \n",
    "    s = 1\n",
    "    \n",
    "    for i in range(n+1):\n",
    "        s = s- (np.exp(-x)*(x**i))/np.math.factorial(i)\n",
    "    \n",
    "    return s"
   ]
